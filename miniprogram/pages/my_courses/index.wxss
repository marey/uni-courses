.sel-course{
  width:100%;
  margin-top:10rpx;
  height:80rpx;
  border-radius:5px; 
  border:solid 1px #f0f0f0;
  text-align: center;
  line-height: 80rpx;
  box-shadow: 0px 3px 5px #f0f0f0;
  background-color: #fff;
}
.sel-course::after{
  content:'';
  height:30rpx;
  width:19rpx;
  float:right;
  margin-top:25rpx;
  margin-right:30rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.sel-course text{

}
.practice-box{
  margin-top:20rpx;
}
.practice-item{
  display: inline-block;
  width:345rpx;
  height:60rpx;
  padding-top:140rpx;
  font-size:30rpx;
  background-color: #fff;
  border-radius: 8rpx;
  border:solid 1px #f0f0f0;
  box-shadow: 0px 3px 5px #f0f0f0;
  background-position: 142rpx 50rpx;
  background-repeat: no-repeat;
  background-size: 60rpx 60rpx;
  text-align: center;
}
.practice-box .special{
  margin-right:10rpx;
  background-image:url(data:image/png;base64,);
}
.practice-box .chapter{
  background-image: url(data:image/png;base64,);
}
.practice-box .random{
  margin-right:10rpx;
  margin-top:10rpx;
  background-image:url(data:image/png;base64,);
}
.practice-box .order{
  margin-top:10rpx;
  background-image:url(data:image/png;base64,);
}
.secret-book{
  margin-top:40rpx;
}
.secret-book .title{
  text-align: left;
}
.secret-book .book-box{
  margin-top:20rpx;
  height:400rpx;
  width:710rpx;
  border-radius: 8rpx;
  border:solid 1px #f0f0f0;
  background-color: #fff;
  box-shadow: 0px 2px 5px #f0f0f0;
}
.secret-book .book-item{
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow:hidden;
  line-height: 100rpx;
  margin:0 20rpx;
  border-bottom:solid 1px #f0f0f0;
}