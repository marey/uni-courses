.answer-box{
  min-height:500rpx;
  margin-top:30rpx;
  padding:30rpx 20rpx;
}
.marginBottom{
  margin-bottom: 120rpx;
}
.question{
  margin-top:20rpx;
  font-size:30rpx;
  font-weight: bold;
}
.single{
  display: inline-block;
  padding:6rpx;
  border:solid 1px #7DBDE6;
  border-radius: 6rpx;
  color:#7DBDE6;
}
.option-box{
  margin-top:50rpx;
  counter-reset:counterli;
}
.option-item{
  counter-increment: counterli;
  margin-top:40rpx;
  font-size:30rpx;
}
.option-item::before{
  content: counter(counterli,upper-alpha);
  display: inline-block;
  margin-right:15rpx;
  background-color: #D8D8D8;
  padding:10rpx 12rpx;
}
.option-box .success::before{
  content: '';
  position: relative;
  top:10rpx;
  width:20rpx;
  height:20rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.option-box .error::before{
  content: '';
  position: relative;
  top:10rpx;
  width:20rpx;
  height:20rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.info-box{
  position: fixed;
  bottom:0;
  background-color:#fff;
  width:100%;
  border-top:solid 1px #f0f0f0;
}
.tar-box{
  min-height:70rpx;
  padding-top:30rpx;
}
.count-box{
  display: inline-block;
  margin-right:60rpx;
}
.count-box .right-logo{
  display: inline-block;
  background-color:#000;
  vertical-align: middle;
  width:40rpx;
  height:40rpx;
  margin-left:40rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.info-box .count-item{
  display: inline-block;
  line-height: 40rpx;
  margin-left:30rpx;
  vertical-align: middle;
}
.count-box .error-logo{
  display: inline-block;
  background-color:#000;
  vertical-align: middle;
  width:40rpx;
  height:40rpx;
  background-size: 100% 100%;
  background-image: url(data:image/png;base64,);
}
.questioncount-box{
  float:right;
  width:200rpx;
}
.question-logo{
  display: inline-block;
  vertical-align: middle;
  width:60rpx;
  height:40rpx;
  background-size: 100%;
  background-image: url(data:image/png;base64,);
}
.question-index-box{
  counter-reset:counterli;
  max-height: 500rpx;
  border-top:solid 1px #cdcdcd;
  margin-bottom:30rpx;
  padding-left:20rpx;
}
.index-item{
  width:90rpx;
  height:90rpx;
  margin-top:30rpx;
  display: inline-block;
  margin-right:60rpx;
  border-radius: 50%;
  text-align: center;
  border:solid 1px #9B9B9B;
  counter-increment: counterli;
}
.index-item:nth-child(5n){
  margin-right:0;
}
.index-item::before{
  content: counter(counterli);
  margin-top:30rpx;
  display: inline-block;
  font-size:38rpx;
}
.question-index-box .success{
  color:#77D887;
  border-color:#77D887;
}
.question-index-box .error{
  color:#D00226;
  border-color:#D00226;
}
.zz-box{
  position: absolute;
  top:0;bottom: 0;right: 0;left: 0;
  background-color:#000;
  opacity: 0.6;
}
.question-explain{
  padding:20rpx 30rpx 40rpx 30rpx;
  margin-top:30rpx;
  font-size:30rpx;
  margin-bottom:120rpx;
}
.question-explain .title{
  color:#9B9B9B;
  font-size:30rpx;
  text-align: center
}
.question-explain .midden-line{
  vertical-align: middle;
  display: inline-block;
  width:50rpx;
  border-top:solid 1px #9B9B9B;
}
.question-explain .midden-line:first-child{
  margin-right: 15rpx;
}
.question-explain .midden-line:last-child{
  margin-left:15rpx;
}
.right-answer{
  margin-top:20rpx;
}
.detail{
  margin-top:15rpx;
}